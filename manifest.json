{
    "name": "Nazar",
    "description": "Cybersecurity Tool",
    "version": "1.0.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/nazar-16.png",
            "32": "/images/nazar-32.png",
            "48": "/images/nazar-48.png"
        }
    },
    "icons": {
        "16": "/images/nazar-16.png",
        "32": "/images/nazar-32.png",
        "48": "/images/nazar-48.png"
    }
}